#include <iostream>
using namespace std;

int main() {
    int T;
    cin >> T;

    for (int i = 0; i < T; i++) {
        int B, C; 
        cin >> B >> C;

        if (B < C ) {
            cout << "BIKE" << endl;
        } else if (B > C) {
            cout << "CAR" << endl;
        } else {
            cout << "SAME" << endl;
        }
    }

    return 0;
}
